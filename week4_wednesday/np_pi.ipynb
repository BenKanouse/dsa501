   "source": [
    "import numpy as np\n",
    "from time import time\n",
    "tic = time()\n",
    "n = int(1e8)\n",
    "rsq = (np.random.rand(2,n)**2).sum(axis=0)\n",
    "approxpi = (rsq<1).sum()*4/n\n",
    "toc = time()\n",
    "print(approxpi, 'at', int(n/(toc-tic)/1e6), 'million points per second')"
   ]
